      "name": "HelloWorld",
      "request": "launch",
      "mainClass": "test.HelloWorld",
      "projectName": "cartago"
    },
    {
      "type": "java",
      "name": "TestController",
      "request": "launch",
      "mainClass": "test.TestController",
      "projectName": "cartago"
    },
    {
      "type": "java",
      "name": "TestLogger",
      "request": "launch",
      "mainClass": "test.TestLogger",
      "projectName": "cartago"
    },
    {
