      "projectName": "yggdrasil",
      "vmArgs": "--add-opens java.base/java.lang=ALL-UNNAMED -jar target/yggdrasil-0.0-shared-ctx-SNAPSHOT.jar -conf src/main/conf/config_hmas_upb.json"
